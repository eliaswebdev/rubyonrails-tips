<p id="notice"><%= notice %></p>
<h1>Motorcycles</h1>
<table class="table">
  <thead>
    <tr>
      <th>Plate</th>
      <th>Color</th>
      <th>Engine</th>
      <th>Gearbox</th>
      <th>Starter system</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @motorcycles.each do |motorcycle| %>
      <tr>
        <td><%= motorcycle.plate %></td>
        <td><%= motorcycle.color %></td>
        <td><%= motorcycle.engine %></td>
        <td><%= motorcycle.gearbox %></td>
        <td><%= motorcycle.starter_system %></td>
        <td><%= link_to 'Show', motorcycle %></td>
        <td><%= link_to 'Edit', edit_motorcycle_path(motorcycle) %></td>
        <td><%= link_to 'Destroy', motorcycle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Motorcycle', new_motorcycle_path %>
